{
  "name": "express-ts-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "app/index.js",
  "scripts": {
    "test": "jest --config jest.config.ts --runInBand  --passWithNoTests --detectOpenHandles --forceExit",
    "test:watch": "npm run test -- --watch --runInBand",
    "start": "npm run build && npm run run",
    "run": "node build/server.js",
    "build": "npm run tsc",
    "tsc": "tsc",
    "dev": "DEBUG=http,mail,express:* nodemon src/server.ts",
    "lint": "eslint . --ext .ts",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh:/git@github.com:dvd90/node-ts-express-bp.git"
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://github.com/dvd90/node-ts-express-bp#readme",
  "husky": {
    "hooks": {
      "pre-commit": "npm run test && npm run lint"
    }
  },
  "dependencies": {
    "@sentry/node": "^6.19.1",
    "@types/morgan": "^1.9.2",
    "cors": "^2.8.5",
    "dotenv": "^8.6.0",
    "express": "^4.17.3",
    "express-jwt": "^6.1.1",
    "express-validator": "^6.14.0",
    "helmet": "^4.4.1",
    "jwks-rsa": "^2.0.3",
    "moment": "^2.29.1",
    "mongoose": "^5.13.14",
    "morgan": "^1.10.0",
    "request": "^2.88.2",
    "typescript": "^4.6.2",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.18.12",
    "@types/qs": "^6.9.6",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "eslint": "^7.29.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "morgan-body": "^2.6.5",
    "nodemon": "^2.0.7",
    "prettier": "^2.6.0",
    "supertest": "^6.2.2",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3"
  }
